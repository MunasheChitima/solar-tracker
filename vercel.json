{
  "version": 2,
  "builds": [
    {
      "src": "SolarGainESP32-web/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "SolarGainESP32-web/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "SolarGainESP32-web/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "SolarGainESP32-web/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
